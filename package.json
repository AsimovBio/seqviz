{
  "name": "seqviz",
  "version": "3.0.42",
  "private": false,
  "description": "A DNA sequence viewer supporting custom, GenBank, FASTA, NCBI accession, and iGEM input",
  "scripts": {
    "start": "esbuild ./src/index.js --bundle --servedir=./src",
    "build": "rm -rf ./dist && esbuild ./src/viewer.js --bundle --outfile=./dist/seqviz.min.js --minify --analyze --target=chrome60,firefox55",
    "demo": "cd demo && npm run deploy && cd .. && git add . && git commit -m \"Update demo\"",
    "format": "npx prettier ./src/**/* --config package.json --write",
    "markdown": "node ./config/scripts/markdown.js ./README.md",
    "update": "npm run build && npm run markdown && git add . && git commit --amend -C HEAD && npm publish",
    "major": "npm run test:ci && npm version major && npm run update",
    "minor": "npm run test:ci && npm version minor && npm run update",
    "patch": "npm run test:ci && npm version patch && npm run update",
    "test": "node ./config/scripts/test.js",
    "test:ci": "CI=true npm run test && npm run build"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "bufferpack": "0.0.6",
    "events": "^3.3.0",
    "node-fetch": "^2.6.1",
    "prop-types": "^15.7.2",
    "react-sizeme": "^2.6.12",
    "string_decoder": "^1.3.0",
    "timers": "^0.1.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "babel-jest": "^27.3.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-imports": "^2.0.0",
    "camelcase": "^5.3.1",
    "core-js": "^3.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "esbuild": "^0.13.8",
    "jest": "^27.3.0",
    "jest-watch-typeahead": "^0.6.1",
    "prettier": "^1.19.1",
    "react": "^16.0.0 || ^17.0.0",
    "react-dom": "^16.0.0 || ^17.0.0",
    "regenerator-runtime": "^0.13.7",
    "xml-comment-api": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0",
    "react-dom": "^16.0.0 || ^17.0.0"
  },
  "keywords": [
    "seqviz",
    "DNA",
    "viewer",
    "sequence viewer",
    "vector viewer",
    "sequence visualizer",
    "plasmid",
    "plasmid viewer"
  ],
  "homepage": "https://github.com/Lattice-Automation/seqviz",
  "bugs": {
    "url": "https://github.com/Lattice-Automation/seqviz/issues",
    "email": "contact@latticeautomation.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lattice-Automation/seqviz.git"
  },
  "license": "MIT",
  "author": "Lattice Automation (https://latticeautomation.com/)",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/index.js",
  "unpkg": "dist/seqviz.min.js",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "babel-plugin-module-resolver",
      "babel-plugin-transform-imports"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "modulePaths": [
      "<rootDir>/src"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setup.js"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "verbose": true,
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "contributors": [
    {
      "name": "Joshua Timmons",
      "email": "joshuatimmons1@gmail.com",
      "url": "https://github.com/JJTimmons"
    },
    {
      "name": "Kevin LeShane",
      "email": "kevin@latticeautomation.com",
      "url": "https://github.com/leshane"
    },
    {
      "name": "Raymond Chung",
      "url": "https://github.com/RazzYoshi"
    }
  ]
}

{
  "name": "@asimov/kernel-universal",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rimraf packages/**/.next",
    "dev": "yarn workspaces foreach -vip run dev",
    "storybook": "yarn dev & yarn workspace @asimov/kernel-storybook run storybook",
    "debug": "yarn workspaces foreach -vip run --inspect dev",
    "build": "yarn workspaces foreach -vip run build",
    "start": "yarn workspaces foreach -vip run start",
    "type-check": "yarn workspaces foreach -vip run type-check",
    "format": "yarn workspaces foreach -vip run format",
    "lint": "yarn workspaces foreach -vip run lint",
    "test": "yarn workspaces foreach -vip run test",
    "test-all": "yarn workspaces foreach -vip run test-all",
    "generate": "graphql-codegen --config codegen.yml",
    "postinstall": "husky install",
    "pre-commit-lint": "yarn type-check && yarn lint-staged"
  },
  "npmClient": "yarn",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/runtime": "^7.15.4",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-codegen/typescript-document-nodes": "2.1.4",
    "@graphql-codegen/typescript-graphql-files-modules": "2.1.0",
    "@graphql-codegen/typescript-graphql-request": "^4.1.4",
    "@graphql-codegen/typescript-operations": "^2.1.5",
    "@jest/types": "^27.2.4",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@yarnpkg/pnpify": "^3.0.1-rc.7",
    "babel-eslint": "^10.1.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest-dom": "latest",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "graphql-codegen-plugin-typescript-swr": "^0.6.1",
    "graphqurl": "^1.0.1",
    "husky": "^7.0.2",
    "jest": "^27.3.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "weak-napi": "^2.0.2",
    "webpack": "^5.55.1"
  },
  "dependencies": {
    "@radix-ui/react-toolbar": "^0.1.1",
    "graphql": "^15.6.0",
    "next": "^11.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "resolutions": {
    "graphql-codegen-plugin-typescript-swr": "patch:graphql-codegen-plugin-typescript-swr@npm:0.6.1#.yarn/patches/graphql-codegen-plugin-typescript-swr-npm-0.6.1-e5f6e8789c"
  }
}

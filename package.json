{
  "name": "seqviz",
  "version": "1.0.12",
  "description": "DNA sequence viewer supporting custom, GenBank, FASTA, NCBI accession, and iGEM part input",
  "keywords": [
    "seqviz",
    "DNA",
    "viewer",
    "sequence viewer",
    "vector viewer",
    "sequence visualizer",
    "plasmid",
    "plasmid viewer"
  ],
  "author": "Lattice Automation (https://latticeautomation.com/)",
  "contributors": [
    "Joshua Timmons <jtimmons@latticeautomation.com> (https://github.com/JJTimmons)",
    "Kevin LeShane <kevin@latticeautomation.com> (https://github.com/leshane)",
    "Raymond Chung (https://github.com/RazzYoshi)"
  ],
  "homepage": "https://github.com/Lattice-Automation/seqviz/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lattice-Automation/seqviz.git"
  },
  "bugs": {
    "url": "https://github.com/Lattice-Automation/seqviz/issues",
    "email": "contact@latticeautomation.com"
  },
  "license": "MIT",
  "main": "dist/seqviz.min.js",
  "private": false,
  "scripts": {
    "start": "PORT=8000 react-scripts start",
    "build": "rm -rf ./dist && webpack --config webpack.config.js --display-error-details && node scripts/editMD.js ./README.md && node scripts/editMD.js ./CODE_OF_CONDUCT.md",
    "demo": "cd demo && npm install && npm run deploy && cd ../..",
    "deploy": "PKG_VERSION=$(node -pe \"require('./package.json').version\") && aws s3 cp dist/seqviz.cdn.js s3://lattice-cdn-s3/libs/seqviz/\"$PKG_VERSION\"/ && npm run demo",
    "major": "npm version major && npm run build && git add . && git commit --amend -C HEAD && npm publish && npm run deploy",
    "minor": "npm version minor && npm run build && git add . && git commit --amend -C HEAD && npm publish && npm run deploy",
    "patch": "npm version patch && npm run build && git add . && git commit --amend -C HEAD && npm publish && npm run deploy",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "bufferpack": "0.0.6",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react-router-dom": "^5.0.0",
    "react-sizeme": "^2.6.4",
    "shortid": "^2.2.14",
    "string_decoder": "^1.3.0",
    "tinycolor2": "^1.4.1",
    "xml2js": "^0.4.19"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-preset-react": "^6.24.1",
    "core-js": "^3.5.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "image-webpack-loader": "^6.0.0",
    "lodash-webpack-plugin": "^0.11.5",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.3.0",
    "regenerator-runtime": "^0.13.3",
    "typescript": "^3.7.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.2",
    "webpack-node-externals": "^1.7.2",
    "why-did-you-update": "^1.0.6",
    "xml-comment-api": "^1.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead"
  ],
  "sideEffects": [
    "**/*.scss"
  ],
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "plugins": [
      "prettier",
      "react"
    ],
    "rules": {
      "prettier/prettier": "error",
      "react/prop-types": 0
    },
    "extends": [
      "plugin:prettier/recommended",
      "prettier/react",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2016,
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "babel-plugin-module-resolver",
      "babel-plugin-transform-imports",
      "lodash"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/fileMock.js",
      "\\.(css|scss|less)$": "<rootDir>/scripts/styleMock.js"
    }
  }
}
